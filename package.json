{
  "name": "key-streaks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.11.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/styles": "4.11.3",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.8.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-scripts": "4.0.3",
    "web-vitals": "1.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@types/jest": "26.0.21",
    "@types/node": "15.0.2",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "dependency-cruiser": "9.23.3",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-compat": "3.9.0",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-jest-formatting": "2.0.1",
    "eslint-plugin-json-format": "2.0.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-promise": "4.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-react-redux": "3.3.2",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-sonarjs": "0.6.0",
    "eslint-plugin-unicorn": "28.0.2",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "standard-version": "9.1.1",
    "typescript": "4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{js,json}\"",
    "format": "prettier --write \"src/**/*.js\"",
    "cruise-deps": "depcruise \"src/**/*.js\"",
    "cm": "git-cz",
    "release": "standard-version"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "CURRENT_BRANCH=$(git branch --show-current); if [ $CURRENT_BRANCH = 'master' ]; then echo 'Error: you are not allowed to push to the master branch'; (exit 1) ; fi"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "npm run lint",
      "npm run cruise-deps"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
